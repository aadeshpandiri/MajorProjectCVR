<!DOCTYPE html>
<html>

<head>
  <title>Faculty Address</title>
  <link rel="stylesheet" type="text/css"
    href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.0.2/css/bootstrap.css" />
  <link rel="stylesheet" type="text/css"
    href="https://cdnjs.cloudflare.com/ajax/libs/bootswatch/5.0.2/paper/bootstrap.min.css" />
  <link rel="stylesheet" type="text/css"
    href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.css" />
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <script type="text/javascript"
    src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.0.2/js/bootstrap.min.js"></script>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"></script>
  <style>
    /* Custom style */
    .navbar {
      margin-bottom: 1rem;
    }
  </style>
  <style id="appnedcss"></style>
</head>

<body>

  <div class="container">
    <div class="m-4">
      <nav class="navbar navbar-expand-lg navbar-dark bg-primary">
        <div class="container-fluid">
          <a href="#" class="navbar-brand">FMS</a>
          <button type="button" class="navbar-toggler" data-bs-toggle="collapse" data-bs-target="#navbarCollapse2">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarCollapse2">
            <div class="navbar-nav">
              <a href="/" class="nav-item nav-link active">Home</a>
              <a href="/file" class="nav-item nav-link active">Bulk UI</a>
              <a href="/filter" class="nav-item nav-link active">Filter</a>
              <a href="/test" class="nav-item nav-link active">Add Records</a>
              <a href="/viewrecords" class="nav-item nav-link active">View Records</a>
            </div>

          </div>
        </div>
      </nav>



      <div class="mx-auto" style="max-width: 100%">
        <center>
          <h2>
            <%= title %>
          </h2>
        </center>
        <p></p>
        <center><button class="button button2 btn btn-success" id="CSV">Download Table</button></center>
        </br>
        <div class="container">
          <div id="chkbtn">
            <div class="row">
              <div class="col">
                <div class="form-check-inline form-switch">
                  <input class="theader1 form-check-input" type="checkbox" id="flexSwitchCheckDefault" checked
                    name="theader1">
                  <label class="form-check-label" for="flexSwitchCheckDefault">Faculty_ID</label>
                </div>
              </div>
              <div class="col">
                <div class="form-check-inline form-switch">
                  <input class="form-check-input theader2" type="checkbox" id="flexSwitchCheckChecked" checked
                    name="theader2">
                  <label class="form-check-label" for="flexSwitchCheckChecked">House_No</label>
                </div>
              </div>
              <div class="col">

                <div class="form-check-inline form-switch">
                  <input class="form-check-input theader3" type="checkbox" id="flexSwitchCheckChecked" checked
                    name="theader3">
                  <label class="form-check-label" for="flexSwitchCheckChecked">Street_Name</label>
                </div>
              </div>
              
            </div>
            <div class="row">
              <div class="col">
                <div class="form-check-inline form-switch">
                  <input class="form-check-input theader4" type="checkbox" id="flexSwitchCheckChecked" checked
                    name="theader4">
                  <label class="form-check-label" for="flexSwitchCheckChecked">Area</label>
                </div>
              </div>
              <div class="col">
                <div class="form-check-inline form-switch">
                  <input class="form-check-input theader5" type="checkbox" id="flexSwitchCheckChecked" checked
                    name="theader5">
                  <label class="form-check-label" for="flexSwitchCheckChecked">Address_Line1</label>
                </div>
              </div>
               <div class="col">
                <div class="form-check-inline form-switch">
                  <input class="form-check-input theader6" type="checkbox" id="flexSwitchCheckChecked" checked
                    name="theader6">
                  <label class="form-check-label" for="flexSwitchCheckChecked">Address_Line2</label>
                </div>
              </div>
            </div>

            <div class="row">
             
              <div class="col">
                <div class="form-check-inline form-switch">
                  <input class="form-check-input theader7" type="checkbox" id="flexSwitchCheckChecked" checked
                    name="theader7">
                  <label class="form-check-label" for="flexSwitchCheckChecked">District</label>
                </div>
              </div>
              <div class="col">
                <div class="form-check-inline form-switch">
                  <input class="form-check-input theader8" type="checkbox" id="flexSwitchCheckChecked" checked
                    name="theader8">
                  <label class="form-check-label" for="flexSwitchCheckChecked">State</label>
                </div>
              </div>
              <div class="col">
                <div class="form-check-inline form-switch">
                  <input class="form-check-input theader9" type="checkbox" id="flexSwitchCheckChecked" checked
                    name="theader9">
                  <label class="form-check-label" for="flexSwitchCheckChecked">PIN_Code</label>
                </div>
              </div>

            </div>
          </div>
        </div>

        <p></p>
        <center>
          <p></p>
          <div style="overflow-x: auto">
            <table id="data" class="table table-striped table-bordered table-sm table-warning border border-dark"
              cellspacing="0" width="100%">
              <thead>
                <tr>
                  <th class="theader1" id="theader1">Faculty Id</th>
                  <th class="theader2" id="theader2">House Number</th>
                  <th class="theader3" id="theader3">Street Name</th>
                  <th class="theader4" id="theader4">Area</th>
                  <th class="theader5" id="theader5">Address Line 1</th>
                  <th class="theader6" id="theader6">Address Line 2</th>
                  <th class="theader7" id="theader7">District</th>
                  <th class="theader8" id="theader8">State</th>
                  <th class="theader9" id="theader9">PinCode</th>

                </tr>
              </thead>
              <tbody>
                <% users.forEach(function(user){ %>
                  <tr>
                    <td class="theader1" id="theader1">
                      <%= user.Faculty_ID %>
                    </td>
                    <td class="theader2" id="theader2">
                      <%= user.House_number %>
                    </td>
                    <td class="theader3" id="theader3">
                      <%= user.Street_name %>
                    </td>
                    <td class="theader4" id="theader4">
                      <%= user.Area %>
                    </td>
                    <td class="theader5" id="theader5">
                      <%= user.Address_line1 %>
                    </td>
                    <td class="theader6" id="theader6">
                      <%= user.Address_line2 %>
                    </td>
                    <td class="theader7" id="theader7">
                      <%= user.District %>
                    </td>
                    <td class="theader8" id="theader8">
                      <%= user.State %>
                    </td>
                    <td class="theader9" id="theader9">
                      <%= user.PinCode %>
                    </td>

                  </tr>
                  <% }); %>
              </tbody>
            </table>
          </div>
      </div>
    </div>
  </div>
  <script>

    $(document).ready(function (e) {
      $("input:checkbox:not(:checked)").each(function () {
        var apndcss = "";
        var column = "table ." + $(this).attr("name");
        apndcss += column + "{display:none;}";
        $("#appnedcss").html(apndcss);
        console.log(apndcss);
      });

      $("#chkbtn").on("change", "input", function () {
        var apndcss = "";
        $("#chkbtn input:checkbox").each(function () {
          if ($(this).is(":not(:checked)")) {
            var column = "table ." + $(this).attr("name");
            apndcss += column + "{display:none;}";
          }
        });
        $("#appnedcss").html(apndcss);
      });
    });

    //Export As CSV
    function download_csv(csv, filename) {
      var csvFile;
      var downloadLink;

      // CSV FILE
      csvFile = new Blob([csv], { type: "text/csv" });

      // Download link
      downloadLink = document.createElement("a");

      // File name
      downloadLink.download = filename;

      // We have to create a link to the file
      downloadLink.href = window.URL.createObjectURL(csvFile);

      // Make sure that the link is not displayed
      downloadLink.style.display = "none";

      // Add the link to your DOM
      document.body.appendChild(downloadLink);

      // Lanzamos
      downloadLink.click();
    }

    function export_table_to_csv(html, filename) {
      var csv = [];
      var rows = document.querySelectorAll("table tr");

      for (var i = 0; i < rows.length; i++) {
        var row = [],
          cols = rows[i].querySelectorAll("td, th");

        for (var j = 0; j < cols.length; j++)
          if ($(cols[j]).is(":visible")) {
            row.push(
              cols[j].innerText[0] == "0"
                ? "'" + cols[j].innerText
                : cols[j].innerText
            );
          }
        csv.push(row.join(","));
      }

      // Download CSV
      download_csv(csv.join("\n"), filename);
    }

    document.querySelector("#CSV").addEventListener("click", function () {
      var html = document.querySelector("table").outerHTML;
      export_table_to_csv(html, "Code_Export.csv");
    });
  </script>
</body>

</html>
