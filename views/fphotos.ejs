<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title><%= title %></title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
    />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css"
    />
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"></script>
    <script
      type="text/javascript"
      src="https://unpkg.com/xlsx@0.15.1/dist/xlsx.full.min.js"
    ></script>
    <style>
      /* Custom style */
      .navbar {
        margin-bottom: 1rem;
      }
    </style>
  </head>

  <body>
    <div class="container">
      <div class="m-4">
        <nav class="navbar navbar-expand-lg navbar-dark bg-primary">
          <div class="container-fluid">
            <a href="#" class="navbar-brand">FMS</a>
            <button
              type="button"
              class="navbar-toggler"
              data-bs-toggle="collapse"
              data-bs-target="#navbarCollapse2"
            >
              <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarCollapse2">
              <div class="navbar-nav">
                <a href="/" class="nav-item nav-link active">Home</a>
                <a href="/file" class="nav-item nav-link active">Bulk UI</a>
                <a href="/filter" class="nav-item nav-link active">Filter</a>
                <a href="/test" class="nav-item nav-link active">Add Records</a>
                <a href="/viewrecords" class="nav-item nav-link active"
                  >View Records</a
                >   
              </div>
            </div>
          </div>
        </nav>
        <div class="mx-auto" style="max-width: 450px">
          <center><h2><%= title %></h2></center>

          <p></p>
          
          <p></p>
         
           <div class="row">
          
          <% users.forEach(function(user){ %>
             <div class="col">
            <div class="card">
                <div class="card-header">Faculty CSE</div>
              <img
                class="card-img-top"
                src='<%= user.Link  %>'
                alt="Card image cap"
              />
              
              <div class="card-body">
                <h5 class="card-title"><%= user.Name %></h5>
                <p class="card-text"><%= user.Designation %></p>
              </div>
            </div>
            </div>
       
          
              
              <% }); %>
          </div>
          
       
          
        </div>
      </div>
    </div>

    <script>
      function ExportToExcel(type, fn, dl) {
        var elt = document.getElementById("tbl_exporttable_to_xls");
        var wb = XLSX.utils.table_to_book(elt, { sheet: "sheet1" });
        return dl
          ? XLSX.write(wb, { bookType: type, bookSST: true, type: "base64" })
          : XLSX.writeFile(wb, fn || "datasheet." + (type || "xlsx"));
      }
    </script>
  </body>
</html>
